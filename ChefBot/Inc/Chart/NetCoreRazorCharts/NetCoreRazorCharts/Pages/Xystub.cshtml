@page
@model XystubModel
@{ Layout = null; }

<!DOCTYPE html>
<html>
<head>
    <title>Simple Clickable XY Chart Handler</title>
</head>
<body style="margin:5px 0px 0px 5px">
<div style="font:bold 18pt verdana;">
    Simple Clickable XY Chart Handler
</div>
<hr style="border:solid 1px #000080" />
<div style="font:10pt verdana;">
<b>You have clicked on the following chart element :</b><br />
<ul>
    <li>Data Set : @Request.Query["dataSetName"]</li>
    <li>X Position : @Request.Query["x"]</li>
    <li>X Label : @Request.Query["xLabel"]</li>
    <li>Data Value : @Request.Query["value"]</li>
</ul>
</div>
</body>
</html>
